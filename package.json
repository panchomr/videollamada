{
  "name": "kollokvium",
  "version": "1.7.9",
  "description": "",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coloquium/kollokvium.git",
    "directory": "dist"
  },
  "author": {
    "name": "Team Kollokvium",
    "url": "https://github.com/coloquium"
  },
  "contributors": [
    {
      "name": "Magnus Thor",
      "url": "https://github.com/MagnusThor"
    },
    {
      "name": "Tibi Covaci",
      "url": "https://github.com/tibor19"
    }
  ],
  "scripts": {
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "webpack --config=webpack.config.js",
    "build:server": "webpack --config=webpack.server.js",
    "build:electron": "build",
    "start": "node dist/server.js",
    "start:secure": "node dist/server.js -s",
    "start:debug": "cross-env WSS_SERVER_URL='wss://dev-wss.kollokvium.net' webpack-dev-server",
    "run-electron": "electron electron-launch.js",
    "pack-electron": "",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "build": {
    "appId": "Kollokvium",
    "category": "",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@microsoft/applicationinsights-web": "^2.5.4",
    "applicationinsights": "^1.7.4",
    "bufferutil": "^4.0.1",
    "express": "^4.17.1",
    "greenscreenstream": "^1.0.1-beta.2",
    "hotkeys-js": "^3.7.6",
    "image-capture": "^0.4.0",
    "mediastreamblender": "^1.0.0-beta.4",
    "thor-io.client-vnext": "^2.0.4-beta.1",
    "thor-io.vnext": "^2.0.5-beta.1",
    "utf-8-validate": "^5.0.2",
    "webrtc-adapter": "^7.5.1",
    "ws": "^7.2.3",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.1",
    "@types/express": "^4.17.6",
    "@types/jquery": "^3.3.35",
    "@types/ws": "^5.1.0",
    "@types/yargs": "^15.0.4",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "electron": "^8.2.0",
    "electron-packager": "^14.2.1",
    "html-webpack-plugin": "^4.0.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
